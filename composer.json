{
    "name": "nbaskoff/beetlecore",
    "description": "The Laravel Framework CMS",
    "keywords": [
        "laravel",
        "admin",
        "administrator",
        "crud",
        "panel"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Nikolay Baskoff",
            "email": "nbaskoff@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
